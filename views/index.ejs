<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/style.css">
    <title>PUB</title>
</head>
<body>

<%- include("./partials/header"); -%>

<!-- main -->
<main>

    <!-- hero section -->
    <section class="hero-section">
        <div class="text">
            <h1>Welkom bij FDND WE LOVE WEB!</h1>
                <h2>Kom een sessie volgen</h2>
                    <h3>waarin een designer/developer uit de praktijk iets komt vertellen en laten zien!</h3>  
                    <!-- <a href="#main"><img width="120em" src="./assets/PikPng.com_pink-hearts-png_2463903.png" alt="heart"> -->
        </div>
            <!-- <img src="./assets/IMG_7910 2.jpg" alt="workshop"> -->
    </section>

    <!-- main content -->
        <!-- navigatie door de website heen -->
    <section id="main"></section>
    <section class="navigation">
        <section class="nav-buttons">
            <nav>
             <ul>
                <button class="sessies-button"><a href="#sessies">We love web sessions</a></button>
                <button class="upcoming-button"><a href="#upcomings">Upcoming sessions</a></button>
                <button class="links-button"><a href="#connections">Links</a></button>
                <button class="aanmelden-button"><a href="#Crud">Aanmelden</a></button>
                <!-- <div class="hart-logo"> <h1>We <img width="30em" src="./assets/PikPng.com_pink-hearts-png_2463903.png"> Web</h1></div> -->
                <!-- <img class="logo-nav" alt="LOGO" width="400em" src="data:image/jpeg;base64,"> -->
            </ul>
            </nav>
        </section>
    </section>

<!-- -----------MAIN----------------------------------- -->

    <section id="Crud"></section>
  
    <section id="main-sessions">
        <section class="sessies"> </section>
<section id="main">
    <div class="main aanmelden">
        <div class="crud">
            <form action="/delete" method="POST">
                <label for="fname">First Name</label>
                <input type="text" id="fname" name="firstname" placeholder="Your name..">
            
                <label for="lname">Last Name</label>
                <input type="text" id="lname" name="lastname" placeholder="Your last name..">
            
                <label for="email">email</label>
                <input type="text" id="email" name="email" placeholder="Your email..">

                <label for="Sessie">Session</label>
                <select id="Sessie" name="Sessie">
                  <option value="We LOVE web CSS">We LOVE web CSS</option>
                  <option value="Coderen als Daan">Coderen als Daan</option>
                  <option value="Frontend Development and Design">Frontend Development and Design</option>
                  <option value="Design for all">Design for all</option>
                </select>
              
                <input type="submit" value="Submit">
            </form>
        </div>
    </div>

<!-- we love web sessies -->
     <div class="main sessies">

        <div class="container">
            <% sessions.forEach(function(session) { %>
                <li>
                  <% sessions.name %>
                 <%- session.sessionId %>
                  <%- session.title %>
                  <a href="/<%- session.speaker %>">Bekijk project</a>
                </li>
            <% }) %>
        </div> 

        <a href="/detail">Detailpagina</a>
        
</section>


<section id ="upcomings"></section>
    <!-- upcoming we love web sessies -->
    <section class="upcoming">

      
        <div class="slider">
            <h1>Upcoming We Love Web Sessies</h1>
  <div class="slides">
    <div id="slide-1">
        <img src="./assets/IMG_7911.jpg" width="300em">
    </div>
    <div id="slide-2">
        <img src="./assets/IMG_7912.jpg" width="300em">
    </div>
    <div id="slide-3">
        <img src="./assets/IMG_7913.jpg" width="300em">
    </div>
    <div id="slide-4">
        <img src="./assets/IMG_7914.jpg" width="300em">
    </div>
    <div id="slide-5">
        <img src="./assets/IMG_7912.jpg" width="300em">
    </div>
  </div>
</div>

    </section>

    <!-- footer -->
    

    <%- include("./partials/footer"); -%>

</main>
    
    

</body>
</html> 
